    {{/each}}

    <div class="slider-nav"></div>
  </div>
  <a href="#" class="prev-slide-2"><i class="fas fa-angle-left fa-3x"></i></a>
  <div class="dots"></div>
  <a href="#" class="next-slide-2"><i class="fas fa-angle-right fa-3x"></i></a>
</div>

