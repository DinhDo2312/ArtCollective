  <!-- we will have a carousel for images, text, and audio -->

<div class ="carousel-container">
    <a href="/create"><button id="create button">Click here</button></a>

  <div id='imgSlick' class = 'img' data-slick='{"slidesToShow": 4, "slidesToScroll": 4}'>Img
    <!-- #each handlebar loop to get data via id -->
    {{#each imageObj}}
        <div class = "slider-item" id="{{this.mediaObj.id}}" data-id="{{this.mediaObj.id}}" data-userId="{{this.mediaObj.userId}}">
          {{this.mediaObj.title}}<a  href="api/media/{{this.mediaObj.id}}"><img class="slider-image" src="{{this.mediaObj.file}}"></a> </div>
    {{/each}}

    <div class="slider-nav"></div>
  </div>
  <a href="#" class="prev-slide-1"><i class="fas fa-angle-left fa-3x"></i></a>
  <div class="dots"></div>
  <a href="#" class="next-slide-1"><i class="fas fa-angle-right fa-3x"></i></a>
</div>


<div class ="carousel-container">
    <a href="/create"><button id="create button">Click here</button></a>
  <div id='txtSlick' class = 'txt'data-slick='{"slidesToShow": 4, "slidesToScroll": 4}'>Txt
